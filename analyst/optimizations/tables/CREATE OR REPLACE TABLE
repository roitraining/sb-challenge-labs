CREATE OR REPLACE TABLE
  optimized.transactions_partitioned_by_date ( `transaction_id` STRING,
    `account_id` STRING,
    `transaction_type` STRING,
    `amount` FLOAT64,
    `transaction_datetime` DATETIME,
    )
PARTITION BY
  date(transaction_datetime) AS (
  SELECT
    *
  FROM
    `sb-challenge-labs.raw.transactions` )